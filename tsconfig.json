{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@theme/*": [
        "src/theme/*"
      ],
      "@navigation/*": [
        "src/navigation/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@constants/*": [
        "src/constants/*"
      ]
    }
  }
}